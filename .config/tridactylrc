" My bindings
bind J tabnext
bind K tabprev
bind j scrollline 3
bind k scrollline -3
bind <C-s> source ~/.config/tridactyl/tridactylrc

" My searchurls
set searchurls.go https://www.google.com/search?q=
set searchurls.s http://localhost/search?q=
set searchurls.aw https://wiki.archlinux.org/?searchq=
set searchurls.yt https://www.youtube.com/search?q=
set searchurls.wk https://www.wikipedia.org/w/index.php?title=Special:Search&search=
set searchurls.swk https://simple.wikipedia.org/wiki/
set searchurls.mdn https://simple.wikipedia.org/wiki/
set searchurls.mdn https://developer.mozilla.org/en-US/search?q=

